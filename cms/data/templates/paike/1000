<script>
var Search=document.getElementById("search");
function Onfocus()
	{
	if(Search.value=="请输入关键字")
		{
			Search.value="";
		}
	}
function Onblur()
	{
if(Search.value=="")
		{
Search.value="请输入关键字";
		}
	}

</script>
