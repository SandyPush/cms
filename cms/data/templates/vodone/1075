<div id="bottombar-wrap-w">
<div class="close-w"><a>关闭</a></div>
<div class="wrap-cont-w">
<div class="cont-minwidth-w">
    <div class="cont-comment-w"><a href="#">查看<i>0</i>条评论</a></div>
    <div class="cont-form-w">
      <div class="form-text-w"><a class="button-w">按钮</a>
      <input type="text" class="textarea-fw"  value="提示的文字" onfocus="if (value =='提示的文字'){value =''}" onblur="if (value ==''){value='提示的文字'}" name="" />
      </div>
    </div>
</div>
</div>
</div>

<script type="text/javascript">
$(document).ready(function(){
  $(".close-w").click(function(){
    $("#bottombar-wrap-w").hide();
  });
});

//到指定位置隐藏
var m_st, m_po = 1010; //滚动到1010像素时显示
$(window).scroll(function () {
m_st = Math.max(document.body.scrollTop || document.documentElement.scrollTop);
if (m_st > m_po) {
showdiv();
}
else
hidediv();
})

function showdiv() {
$('#bottombar-wrap-w').addClass("bottom_bor_display");
}
function hidediv() {
$('#bottombar-wrap-w').removeClass("bottom_bor_display");
}
</script>

</body>
</html>
