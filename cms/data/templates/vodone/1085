<!-- save sort order here which can be retrieved on server on postback -->
	<input name="list1SortOrder" type="" />

	<script type="text/javascript">
		$("#list1, #list2").dragsort({ itemSelector:"li",dragSelector: "div", dragBetween: true, dragEnd: saveOrder, placeHolderTemplate: "<li class='placeHolder'><div></div></li>" });
		
		function saveOrder() {
			var data = $("#list2 li").map(function() {
                    console.log($(this).attr("id"));
                    return $(this).children().html(); 
                }).get();
			$("input[name=list1SortOrder]").val(data.join("|"));
		};
	</script>

