};

function showDiv(lastIndex){
   if(lastIndex>=<!--#echo $album->photos;#-->){
     $("#picEnd").css('display','');
     return false;
   }else{
     $("#picEnd").css('display','none');
     return true;
   }
}

var Search=document.getElementById("search");
function Onfocus()
	{
	if(Search.value=="请输入关键字")
		{
			Search.value="";
		}
	}
function Onblur()
	{
if(Search.value=="")
		{
Search.value="请输入关键字";
		}
	}

//记录
	$("#firstpane p.CTmenu_head").click(function()
    {
		$(this).css({backgroundImage:""}).next("div.CTmenu_body").slideToggle(0).siblings("div.CTmenu_body").slideUp("slow");
       	$(this).siblings().css({backgroundImage:""});
        data=cookie.get('playHistory');
		dataArray=data.split(',');
		htmlString = '';
		for(i in dataArray){
			dataInfo=dataArray[i].split('|');
			title=dataInfo[0];
			url=dataInfo[1];
			if(title){
				htmlString+='<a href="'+url+'" target="_blank">'+subString(title,20,'')+'</a>';
			}
		}
		if(dataArray.length>0){
			$(this).siblings('div').empty();
			$(this).siblings('div').append(htmlString);
		}
	});

