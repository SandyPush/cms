//自动轮播
var imageAutoPlay;
var urlArray=location.href.split('#');
function play(){
    currentIndex=parseInt($("#currentIndex").text());
    url=urlArray[0];
    cutTime=urlArray[1] ? parseInt(urlArray[1]) : 10;
    imageAutoPlay=window.setTimeout(function(){
        YAO.YAlbum.prototype.Next(cutTime);
        //newUrl=url.replace(/(_[0-9]+)?\.shtml/i,'_'+(currentIndex+1)+'.shtml');
        //location.href=newUrl+"#"+cutTime;
    },cutTime*1000);
    return imageAutoPlay;
};
play();

function stop(){
  window.clearTimeout(imageAutoPlay);
}
function setCutTime(cutTime){
    location.href=urlArray[0]+"#"+cutTime;
    nextUrlArray=$("#carousel_next_photo").attr("href").split('#');
    $("#carousel_next_photo").attr("href",nextUrlArray[0]+"#"+cutTime);
}

$("#play").click(function(){
    if($(this).attr("class")=='play_2'){
      $(this).attr("class","play_1");
      play();
    }else{
      $(this).attr("class","play_2");
      stop();
    }
});
$(".ina_miao a").each(function(){
    $(this).click(function(){
      miao=$(this).attr("title");
      setCutTime(miao);
    });
});
</script>
