<script>

$(document).ready(function(){
    photoNumMatch=location.href.match(/[0-9]+_([0-9]+)\.shtml/);
    photoNum=photoNumMatch ? parseInt(photoNumMatch[1]) : 1;
    var Album = new YAO.YAlbum();
    Album.lastIndex=photoNum;
    YAO.removeClass(Album.lastSample, 'current');
	YAO.addClass(Album.oSamples[photoNum-1], 'current');
	Album.lastSample = Album.oSamples[photoNum-1];
    Album.go();  //启动滚轴
    
    $("#currentIndex").text(photoNum);
    
    previousUrl=formatUrl(photoNum-1);
    nextUrl=formatUrl(photoNum+1);
    $("#carousel_previous_photo").attr("href",previousUrl);
    $("#carousel_next_photo").attr("href",nextUrl);
    
    $("#samples_list li").each(function(i){
        $(this).children().attr("href",formatUrl(i+1)); 
    });
    
    sPic=$("#samples_list li").eq(photoNum-1).children("a").children("img").attr("src");
    title=$("#samples_list li").eq(photoNum-1).children("a").attr("title");
    mPic=sPic.replace('_s','_m');
    
    Album.load(mPic);
    $("#carousel_photo").attr("src",mPic);
    $("#carousel_photo_intro").text(title);
});

function formatUrl(num){
    if(num<=0) num=1;
    if(location.href.match(/[0-9]+_([0-9]+)\.shtml/)){
        rgExp=/_[0-9]+\.shtml/;
    }else{
        rgExp=/\.shtml/;
    }
    url=location.href.replace(rgExp,'_'+num+".shtml");
    return url; 
}

YAO.YAlbum.prototype.go = function(){
	var curScreen = Math.ceil(((this.lastIndex + 1) * this.oCarousel.itemWidth) / this.oCarousel.stepWidth) - 1;
	if (curScreen != this.oCarousel.movedNum) {
		this.oCarousel.go(curScreen);
		this.oCarousel.movedNum = curScreen;
		if (this.oCarousel.movedNum !== 0 && YAO.hasClass(this.oCarousel.lnkBtnPrevious, this.BTN_DISABLED_CLASS)) {
			YAO.removeClass(this.oCarousel.lnkBtnPrevious, this.BTN_DISABLED_CLASS);
		}
		else {
			if (this.oCarousel.movedNum === 0) {
				YAO.addClass(this.oCarousel.lnkBtnPrevious, this.BTN_DISABLED_CLASS);
			}
		}
		if (this.oCarousel.movedNum !== this.oCarousel.maxMovedNum && YAO.hasClass(this.oCarousel.lnkBtnNext, this.BTN_DISABLED_CLASS)) {
			YAO.removeClass(this.oCarousel.lnkBtnNext, this.BTN_DISABLED_CLASS);
		}
		else {
			if (this.oCarousel.movedNum === this.oCarousel.maxMovedNum) {
				YAO.addClass(this.oCarousel.lnkBtnNext, this.BTN_DISABLED_CLASS);
			}
		}
	}
};

YAO.Carousel.prototype.go = function(steps){
	var scrollWidth = 0, scrollHeight = 0;
	if (this.derection === 'H') {
		if (this.stepWidth) {
			scrollWidth = -(this.stepWidth * steps);
		}
		else {
			scrollWidth = -(this.itemWidth * steps);
		}
	}
	else {
		if (this.stepHeight) {
			scrollHeight = -(this.stepHeight * steps);
		}
		else {
			scrollHeight = -(this.itemHeight * steps);
		}
	}
	$("#samples_list").css("left",scrollWidth);
};
	
YAO.YAlbum.prototype.Previous = function(){
    location.href=$("#carousel_previous_photo").attr("href");
    /*
    if (this.lastIndex !== 0) {
		$("#currentIndex").text(parseInt($("#currentIndex").text())-1);
                this.lastIndex -= 1;
		if (YAO.hasClass(this.btnNext, this.BTN_DISABLED_CLASS)) {
			YAO.removeClass(this.btnNext, this.BTN_DISABLED_CLASS);
		}
		if (this.lastIndex >= 1) {
			this.btnPrevious.href = this.oSamples[this.lastIndex - 1].href;
		}
		if (this.lastIndex < 0) {
			this.lastIndex = 0;
			YAO.addClass(this.btnPrevious, this.BTN_DISABLED_CLASS);
		    this.btnPrevious.href = this.oSamples[this.lastIndex].href;
		}
		this.btnNext.href = this.oSamples[this.lastIndex+1].href;
		this.chgPhoto();
	}
    */
};

YAO.YAlbum.prototype.Next = function(cutTime){
    if(showDiv(photoNum)){
        cutTimeStr=location.hash ? '' : "#"+cutTime;
        location.href=$("#carousel_next_photo").attr("href")+cutTimeStr;
    }
    /*    
    if (this.lastIndex < (this.length - 1)) {
		$("#currentIndex").text(parseInt($("#currentIndex").text())+1);
                this.lastIndex += 1;
		if (YAO.hasClass(this.btnPrevious, this.BTN_DISABLED_CLASS)) {
			YAO.removeClass(this.btnPrevious, this.BTN_DISABLED_CLASS);
		}
		if (this.lastIndex <= (this.length - 2)) {
			this.btnNext.href = this.oSamples[this.lastIndex + 1].href;
		}
		if (this.lastIndex > (this.length -1)) {
			this.lastIndex = (this.length - 1);
			YAO.addClass(this.btnNext, this.BTN_DISABLED_CLASS);
			this.btnNext.href = this.oSamples[this.lastIndex].href;
		}
		this.btnPrevious.href = this.oSamples[this.lastIndex-1].href;
		this.lastIndex=6;
        this.chgPhoto();
	}else{
          picNum=this.lastIndex+1;
          showDiv(picNum);
        }
    */
 
};


YAO.YAlbum.prototype.chgPhoto = function(){
    location.href= this.oSamples[this.lastIndex].href;
    /*
	var path = '';
	this.sIntro = this.oSamples[this.lastIndex].title;
