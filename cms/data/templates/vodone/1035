<div id="part00" class="part00 udv-clearfix">   
			<h2 class="tw_title udv-clearfix">
        		<span class="tw_title_cont" id="d_picTit"><!--#echo $album->title;#-->(<span id="currentIndex"></span>/<!--#echo $album->photos;#-->)</span>
