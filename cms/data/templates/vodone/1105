	  				q=$(w);
	  				q.show();
  				}
  				$("#hiddenbox").val(num);
  				$("#buttonbox").click(function(){
  					num+=10;
	  				for(var b=0;b<num;b++){
	  				w=t[b];
	  				q=$(w);
	  				q.show();
					if(num==50){
						$("#buttonbox").hide();
					}
  				}
  				});				
  			});
</script>
