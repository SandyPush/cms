$(".nytabTitle03 li").each(function(){
	var index = $(this).index();
	var r = index + 1;
	if(ind == index) {
		$(this).addClass("ny3tab0"+r+"_cur").removeClass("ny3tab0"+r);
	} else {
	   $(this).addClass("ny3tab0"+r).removeClass("ny3tab0"+r+"_cur");
	}
  });
$(".nypictabCon03 > div").eq(ind).show().siblings().hide();
});
   });  
});
