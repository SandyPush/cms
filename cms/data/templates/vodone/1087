<script>
	$(function(){
	  var o = document.getElementById("ul3");
	  var s = o.innerHTML;
	  var zyFont = $(o).find("font");
	  var zyFontHtml = zyFont[0].innerHTML;
	  var zyLength = zyFontHtml.length;
	  if(zyLength > 47){
		  zyFontHtml = zyFontHtml.substring(0,47) + "...";
		  zyFont.html(zyFontHtml);
	  }
	  var newOHtml = o.innerHTML;
	  var p = parent.document.createElement("span");
	  p.innerHTML = newOHtml;
	  o.innerHTML = "";
	  if(zyLength > 47){
			var n = parent.document.createElement("a");
			n.innerHTML = zyLength > 47 ? "展开▼" : "";
			n.href = "javascript:;";
			n.onclick = function(){
				if (n.innerHTML == "展开▼"){
				  n.innerHTML = "收起▲";
				  p.innerHTML = s;
				}else{
				  n.innerHTML = "展开▼";
				  p.innerHTML = newOHtml;
				}
			}
		   o.appendChild(p);
		   o.appendChild(n);
	  }else{
		   o.appendChild(p);
	  }
	 
	 
	});
  </script>
