  <script>
(function(){
  var o = document.getElementById("ul3");
  var s = o.innerHTML;
  var p = document.createElement("span");
  var n = document.createElement("a");
  p.innerHTML = s.substring(0,104);
  n.innerHTML = s.length > 104 ? "展开▼" : "";
  n.href = "#azk";
  n.class="show_more"
  n.onclick = function(){
    if (n.innerHTML == "展开▼"){
      n.innerHTML = "收起▲";
      p.innerHTML = s;
    }else{
      n.innerHTML = "展开▼";
      p.innerHTML = s.substring(0,104);
    }
  }
  o.innerHTML = "";
  o.appendChild(p);
  o.appendChild(n);
})();
  </script>
       </div>
    </div>
