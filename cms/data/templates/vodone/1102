<script>
$(document).ready(function(){
//导航变色
$(".minisubnav").mouseover(function(){
$(".subnav_toggle").show();
$(".subnav_but").addClass("subnav_but:hover");
$(".banner").css("z-index","-1");
});
$(".minisubnav").mouseout(function(){
$(".subnav_toggle").hide();
$(".subnav_but").removeClass("subnav_but:hover");
$(".banner").css("z-index","1");
});
})
</script>

