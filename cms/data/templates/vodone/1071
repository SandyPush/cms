$(function(){
	var tt_top = $("#tt").offset().top;
	var ox_top = $("#ox").offset().top;
	var aq_top = $("#aq").offset().top;
	var hz_top = $("#hz").offset().top;
	var zv1_top = $("#zv1").offset().top;
        var v1_top = $("#v1").offset().top;
	//alert(tops);
	$(window).scroll(function(){
		var scroH = $(this).scrollTop();
		if(scroH>=v1_top){
			set_cur(".v1");
                }else if(scroH>=zv1_top){
			set_cur(".zv1");
		}else if(scroH>=hz_top){
			set_cur(".hz");
		}else if(scroH>=aq_top){
			set_cur(".aq");
		}else if(scroH>=ox_top){
			set_cur(".ox");
		}else if(scroH>=tt_top){
			set_cur(".tt");
		}
	});
	
});

function set_cur(n){
	if($(".nav a").hasClass("cur")){
		$(".nav a").removeClass("cur");
	}
	$(".nav a"+n).addClass("cur");
}
