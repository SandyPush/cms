<script>

    var listli = "";
	var liLeng = 0;
	var index = 0;
	var scrollTop = 0;
	var windowHeight = 0;
	var task;
$(document).ready(initFuncation);

function initFuncation(){
	$(".leftbar").find("#ptext").hide();
	//$(".lbar_clickbox").find(".lbar_mousetext").show();

	listli = $(".leftbar").parent().find("ul li");
	liLeng = listli.size();
	index = 0;

	windowHeight = $(window).height();
	var crollStatus = true;
	var top = $(listli[liLeng-1]).offset().top;

	$(window).scroll(function () {
         scrollTop = $(window).scrollTop();
		 if(scrollTop + windowHeight >= top && crollStatus == true){
			 crollStatus = false;
             $(".leftbar").css({"position":"fixed", "bottom":"0px"});
		 }
         if(scrollTop + windowHeight < top && crollStatus == false){
		     crollStatus = true;
			 $(".leftbar").css({"position":"relative", "bottom":""});
		 }
		 var div =  $("#bot01");
		 var bot = div.offset().top;
		 if(scrollTop + windowHeight + 50 > bot && crollStatus == false){
		     crollStatus = true;
			 var divTop = (scrollTop + windowHeight + 140 - bot) + "px";
			 $(".leftbar").css({"position":"fixed", "bottom":divTop});
		 }
	 });
    clearInterval(task);
    task = setInterval(intervalTask, 2000);
	//var task = setInterval(function(){}, 1000);

	$(".lbar_clickbox").hide();
	$(".lbar_clickbox").find("#ctext").hide();
	$(".lbar_icon").hover(function(){
		    var top = $(this).offset().top;
			var lbar_clickbox = $(this).parent().find(".lbar_clickbox");
			if(top - scrollTop - 3*windowHeight/5 > 0){
				var lbar_mousetext = $(this).parent().find("#ptext");
				if(lbar_mousetext.css("display") == "block"){
					lbar_clickbox.css({"top":"-180%"});
				}else{
				    lbar_clickbox.css({"top":"-400%"});
				}
				lbar_clickbox.find(".caret").css({"top":"85%"});
			}else{
			    lbar_clickbox.css({"top":"-0%"});
				lbar_clickbox.find(".caret").css({"top":"5%"});
			}
		    clearInterval(task);
	        $(this).parent().find(".lbar_clickbox").show();
			$(this).parent().find(".lbar_clickbox").find("#ctext").show();
			
	}, function(){
		     clearInterval(task);
		     task = setInterval(intervalTask, 2000);
	         $(this).parent().find(".lbar_clickbox").hide();
			 $(this).parent().find(".lbar_clickbox").find("#ctext").hide();
			 $(".lbar_clickbox").hover(function(){
				  clearInterval(task);
			      $(this).show();
				  $(this).find("#ctext").show();
			},function(){
				  clearInterval(task);
		          task = setInterval(intervalTask, 2000);
			      $(this).hide();
				  $(this).find("#ctext").hide();
			})
	});

    $("#ptext span").hover(function(){
				  clearInterval(task);
			},function(){
				  clearInterval(task);
		          task = setInterval(intervalTask, 2000);
			})
	
}

function intervalTask(){
		if(index >= liLeng){
			index = 0;
		}
		//listli.find(".lbar_mousetext").hide();
        //$(listli[index]).find(".lbar_mousetext").show();
		listli.find("#ptext").hide();
        $(listli[index]).find("#ptext").show();
		index++;
}

$(document).ready(function(){
if(location.host=='big5.v1.cn'){
    $(".lang").children('a').html('简体版');
    $(".lang").children('a').attr("href","http://www.v1.cn");
}
});

</script>
