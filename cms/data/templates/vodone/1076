<div class="player" style="height:428px;">
<!--#CMS_MODULE(视频模块NO4)#-->
<!--#CMS_MODULE(code003)#-->
<script type="text/javascript">
//轮播主调函数
var playType='zhuanti';
var lunbo_index = 0;
var li_array = $(".jScrollbar_mask").find("li");
var li_num = li_array.length;
//手动控制播放列表
$("#vidList li").each(function(){
 $(this).click(function(){
    lunbo_index=$(this).index();
    changeNewsInfo();
 })
});

</script>
</div>
<!--视频播放01 end-->

