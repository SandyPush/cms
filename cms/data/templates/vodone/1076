<!--#CMS_MODULE(模板NO1背景/分栏/边框)#-->
<div class="topic_img"><img style=";" src=""></div>
<script type="text/javascript">  
$(function(){  
$("#menu li:last").css("border","none");
})

//tab 新闻列表切换
$(function(){
$(".newlisttabCon div").first().show().siblings().hide();
$(".newlisttabTitle li").click(
function(){
var ind=$(this).index();
$(this).addClass("cur").siblings().removeClass("cur");
$(".newlisttabCon > div").eq(ind).show().siblings().hide();
}
);
});

//tab 详细内容切换
$(function(){
$(".detailtabCon div").first().show().siblings().hide();
$(".detailtabTitle li").click(
function(){
var ind=$(this).index();
$(this).addClass("cur").siblings().removeClass("cur");
$(".detailtabCon > div").eq(ind).show().siblings().hide();
}
);
});

</script> 

