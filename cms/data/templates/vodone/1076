//tab 新闻列表切换
$(function(){
$(".newlisttabCon div").first().show().siblings().hide();
$(".newlisttabTitle li").click(
function(){
var ind=$(this).index();
$(this).addClass("cur").siblings().removeClass("cur");
$(".newlisttabCon > div").eq(ind).show().siblings().hide();
}
);
});

