    <script type="text/javascript">
//新车T型台Tab
	var lb = $("#autoTOut"),
		lb_cur = 1,
		lbp_w = lb.find(".autoTpic").width();
		lb_timer = null;
	t = 1;
	function showLimitBuyProducts(){
		if(lb_cur < 1){
			lb_cur = 2;
		} else if(lb_cur > 2){
			lb_cur = 1;
		}
		$("#autoTJ-lbcp").html(lb_cur);
