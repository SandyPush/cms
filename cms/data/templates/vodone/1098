function indexFocus(strId) {
    var domRoot = document.getElementById('autoFocusContent');
    if (!domRoot) return;
    domRoot.list = [];
    var children = domRoot.childNodes;
    var offset = 0;
    for (var i in children) {
        var domItem = children[i];
        if (domItem && domItem.className == 'autoFodetail') {
            domRoot.list.push(domItem);
            domItem.eoffset = offset;
            offset++;
        }
    }
    var domList = document.getElementById('autoFoIcon-items');
    if (!domList) return;
    domList.innerHTML = '';
    domList.items = [];
