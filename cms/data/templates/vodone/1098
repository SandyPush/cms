    <script type="text/javascript">
//新车T型台Tab
	var lb = $("#autoTOut"),
		lb_cur = 1,
		lbp_w = lb.find(".autoTpic").width();
		lb_timer = null;
	t = 1;
	function showLimitBuyProducts(){
		if(lb_cur < 1){
			lb_cur = 2;
		} else if(lb_cur > 2){
			lb_cur = 1;
		}
		$("#autoTJ-lbcp").html(lb_cur);
		var autoTpic = $("#autoTOut .autoTpic").hide().eq(lb_cur-1).show(),
			ta = autoTpic.find("textarea");
			
		if(ta.length){
			autoTpic.html(ta.val());
		}
	}
	lb_timer = setInterval(function(){
		lb_cur++;
		showLimitBuyProducts();
	}, 4000);
	
	$("#autoTJ-lbn .prev, #autoTJ-lb .autoTbtn-prev").click(function(){
		lb_cur--;
		showLimitBuyProducts();
	});
	$("#autoTJ-lbn .next, #autoTJ-lb .autoTbtn-next").click(function(){
		lb_cur++;
		showLimitBuyProducts();
	});
	$("#autoTJ-lb").hover(function(){
			clearInterval(lb_timer);
			lb_timer = null;
			$("#autoTJ-lb .autoTbtn-prev, #autoTJ-lb .autoTbtn-next").show();
		}, function(){
			lb_timer = setInterval(function(){
				lb_cur++;
				showLimitBuyProducts();
			}, 10000);
			$("#autoTJ-lb .autoTbtn-prev, #autoJ-lb .autoTbtn-next").show();
	});
	

	</script>
<!--新车T型台end-->

