              <!--#CMS_MODULE(汽车六十周年国内媒体评委)#-->
           </ul>
           </div>
           <span class="next"></span> 
        </div>
        <div class="gn-rater">
           <span>国外媒体评委</span>
           <span class="prev-gw"></span>
           <div id="content-gw">
