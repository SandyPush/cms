";i:15;s:9067:"<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>{$cms_page_title}</title>
<link type="text/css" rel="stylesheet" href="http://pic.v1.cn/cms/v1_zj/css/zj.css" />

<script type="text/javascript" src="http://pic.v1.cn/cms/v1_static/common/js/jquery-1.7.2.min.js"></script>
<script type="text/javascript" src="http://pic.v1.cn/cms/v1_zj/js/globalFun.js"></script>
<script type="text/javascript" src="http://pic.v1.cn/cms/v1_zj/js/fy.js"></script>
<script>
    $(function(){  
        var page = 1;  
        var i = 5; //每版放4个图片  
        //向后 按钮  
        $("span.next").click(function(){    //绑定click事件  
             var content = $("div#content");   
             var content_list = $("div#content_list");  
             var v_width = content.width();  
             var len = content.find("dl").length;  
             var page_count = Math.ceil(len / i) ;   //只要不是整数，就往大的方向取最小的整数  
             if( !content_list.is(":animated") ){    //判断“内容展示区域”是否正在处于动画  
                  if( page == page_count ){  //已经到最后一个版面了,如果再向后，必须跳转到第一个版面。  
                    content_list.animate({ left : '0px'}, "slow"); //通过改变left值，跳转到第一个版面  
                    page = 1;  
                  }else{  
                    content_list.animate({ left : '-='+v_width }, "slow");  //通过改变left值，达到每次换一个版面  
                    page++;  
                 }  
             }  
       });  
        //往前 按钮  
        $("span.prev").click(function(){  
             var content = $("div#content");   
             var content_list = $("div#content_list");  
             var v_width = content.width();  
             var len = content.find("dl").length;  
             var page_count = Math.ceil(len / i) ;   //只要不是整数，就往大的方向取最小的整数  
             if(!content_list.is(":animated") ){    //判断“内容展示区域”是否正在处于动画  
                 if(page == 1 ){  //已经到第一个版面了,如果再向前，必须跳转到最后一个版面。  
                    content_list.animate({ left : '-='+v_width*(page_count-1) }, "slow");  
                    page = page_count;  
                }else{  
                    content_list.animate({ left : '+='+v_width }, "slow");  
                    page--;  
                }  
            }  
        });
        //计算内容数量
        tabDiv=$("#tab div");
        $("font[color=red]").html(tabDiv.length);
        
        //计算总播放PV
        var contentPv=0;
        $("#tab div").each(function(i){
            contentPv+=parseInt($(this).find("dd[class=bof]").text());
        });
        $("div[class=bof]").html("总播放量："+contentPv);
    });

    function fresh(dateTime)
    {	
       var startTime= new Date(dateTime);
       var nowTime = new Date();
       //两时间差为毫秒数，除以1000则转换为秒数
       var leftsecond=parseInt((nowTime.getTime()-startTime.getTime())/1000);
       d=parseInt(leftsecond/(3600*24));//计算出相差天数
       d=d>=1 ? d+'天前' : '今天';
       document.write(d);
    }

    var playType='videoAlbum';
</script>
