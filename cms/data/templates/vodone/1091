$.get("/ssi/cnxh.shtml", {}, function(content){
   $('#cnxh').ready(function(){
     $('#cnxh').html();
     content=content.replace(/src="/g,'data-original="');
     $('#cnxh').html(content);
     $.get("/ssi/contad/cnxhyc1.shtml", {}, function(content){
          content=content.replace(/src="/g,'data-original="');
          $('#cnxh').append(content);
     });
     $.get("/ssi/contad/cnxhyc2.shtml", {}, function(content){
          content=content.replace(/src="/g,'data-original="');
          $('#cnxh').append(content);
          $('#cnxh img').lazyload({"effect":"fadeIn","threshold":200});
          checkFF();
     });
   });  
});


</script>
