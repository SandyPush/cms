<script type="text/javascript">
$(document).ready(function(){
  $(".close-w").click(function(){
    $("#bottombar-wrap-w").hide();
  });
});

//到指定位置隐藏
var m_st, m_po = 1010; //滚动到1010像素时显示
$(window).scroll(function () {
m_st = Math.max(document.body.scrollTop || document.documentElement.scrollTop);
if (m_st > m_po) {
showdiv();
}
else
hidediv();
})

function showdiv() {
$('#bottombar-wrap-w').addClass("bottom_bor_display");
$('#tanChuangAd').css("bottom","0px");
$('#closeTC').css("bottom","250px");
}
function hidediv() {
$('#bottombar-wrap-w').removeClass("bottom_bor_display");
$('#tanChuangAd').css("bottom","38px");
$('#closeTC').css("bottom","288px");
}
</script>
<!--页面底部评论 end-->

