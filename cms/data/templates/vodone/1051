<script>
$(function () {
		$(".num2,.num3,.num5,.num6,.num8,").hover( 
							function(){
						$(this).find("span").css({top:"35px",left:"10px"});
						$(this).find("div").css({top:"0px",left:"0px"});
								},
						function(){
						$(this).find("span").css("top","143px")
						$(this).find("div").css({top:"143px",left:"0px"});
								}
								);
		$(".num1").hover( 
							function(){
						$(this).find("div").css({filter:"alpha(opacity=30)",opacity:"0.30"});
								},
						function(){
						$(this).find("div").css({filter:"alpha(opacity=20)",opacity:"0.20"});
								}
								);	
		$(".num4,.num7").hover( 
							function(){
						$(this).find("span").css({background:"url(http://pic.v1.cn/cms/special_channel3/images/icon-tab.png) no-repeat -357px 2px",color:"#ffffff"});
								},
						function(){
						$(this).find("span").css({background:"url(http://pic.v1.cn/cms/special_channel3/images/icon-tab.png) no-repeat -357px -59px",color:"#eeeeee"});
								}
								);
		$(".xcmv ul li").hover( 
							function(){
						$(this).find("span").css({bottom:"0px",left:"0px"});
						$(this).find("div").css({bottom:"0px",left:"0px"});
								},
						function(){
						$(this).find("span").css({bottom:"-25px",left:"0px"});
						$(this).find("div").css({bottom:"-25px",left:"0px"});
								}
								);
			
			})
  
</script>
