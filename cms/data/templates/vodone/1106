<script>
    $(function(){
        $('#focus-slides').slides({
            preloadImage: 'img/loading.gif',
            play: 5000,
            effect: 'slide, fade',
            pause: 2000,
            hoverPause: true,
            animationStart: function(current){
                $('#focus-slides .caption').animate({
                    bottom:-50
                },100);
                if (window.console && console.log) {
                    // example return of current slide number
                    console.log('animationStart on slide: ', current);
                };
            },
            animationComplete: function(current){
                $('#focus-slides .caption').animate({
                    bottom:0
                },200);
                if (window.console && console.log) {
                    // example return of current slide number
                    console.log('animationComplete on slide: ', current);
                };
            },
            slidesLoaded: function() {
                $('#focus-slides .caption').animate({
                    bottom:0
                },200);
            }
        });
        $('#big-slides').slides({
            preloadImage: 'img/loading.gif',
            play: 5000,
            effect: 'slide',
            pause: 2000,
            hoverPause: true,
            slideSpeed: 400,
            generateNextPrev: true,
            generatePagination: false,
            slidesLoaded: function() {
                var items_count = $('#big-slides .pagination li').length;
                for(var i=o;i<items_count;i++){
                    items_width = $(this).outerWidth(true);
                    items_width = items_width++;
                    alert(items_width);
                }
            }
        });
        $('#medium-slides').slides({
            preloadImage: 'img/loading.gif',
            play: 5000,
            effect: 'fade',
            pause: 2000,
            hoverPause: true,
            slideSpeed: 200,
            fadeSpeed: 200,
            generateNextPrev: false,
            generatePagination: false
        });
    });
</script>
<!--跑马灯 end-->
