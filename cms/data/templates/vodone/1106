<script src="js/slides.min.jquery.js"></script>
<script>
    $(function(){
        $('#focus-slides').slides({
            preloadImage: 'img/loading.gif',
            play: 5000,
            effect: 'slide, fade',
            pause: 2000,
            hoverPause: true,
            generateNextPrev: false,
            next: 'focus-slides-next',
            prev: 'focus-slides-prev',
            animationStart: function(current){
                $('#focus-slides .caption').animate({
                    bottom:-50
                },100);
                /*
                if (window.console && console.log) {
                    // example return of current slide number
                    console.log('animationStart on slide: ', current);
                };
                */
            },
            animationComplete: function(current){
                $('#focus-slides .caption').animate({
                    bottom:0
                },200);
                /*
                if (window.console && console.log) {
                    // example return of current slide number
                    console.log('animationComplete on slide: ', current);
                };
                */
            },
            slidesLoaded: function() {
                $('#focus-slides .caption').animate({
                    bottom:0
                },200);
            }
        });
              
        $('#big-slides').slides({
            preloadImage: 'img/loading.gif',
            play: 5000,
            effect: 'slide',
            pause: 2000,
            hoverPause: true,
            slideSpeed: 400,
            generateNextPrev: false,
            next: 'big-slides-next',
            prev: 'big-slides-prev',
            generatePagination: false
        });
        
        var obj = $('#big-slides .pagination'),
            wrapWidth = 0,
            showCount = 7,
            hasNextClicks = 0,
            itemsCount = obj.find('ul li').length,
            allClicks = itemsCount%showCount==0 ? parseInt(itemsCount/showCount)-1 : parseInt(itemsCount/showCount);
        //初始化wrap的的宽
        obj.find('ul li').each(function(){
            wrapWidth += $(this).outerWidth(true);
        })
        obj.find('ul').width(wrapWidth);
        
        if(itemsCount > showCount){
            $('.pagination-next').click(function(){
                if(hasNextClicks < allClicks){
                    hasNextClicks++;
                    warpLeft = -showCount*obj.find('ul li').outerWidth(true)*hasNextClicks;
                    obj.find('ul').animate({
                            marginLeft:warpLeft
                    },500)
                }
            })
            $('.pagination-prev').click(function(){
                if(hasNextClicks>0){
                    hasNextClicks--;
                    obj.find('ul').animate({
                        marginLeft:-showCount*obj.find('ul li').outerWidth(true)*hasNextClicks
                    },500)
                }
            })
        }else{
            obj.find('.pagination-prev').hide();
            obj.find('.pagination-next').hide();
        }
        
        $('#medium-slides').slides({
            preloadImage: 'img/loading.gif',
            play: 5000,
            effect: 'fade',
            pause: 2000,
            hoverPause: true,
            slideSpeed: 200,
            fadeSpeed: 200,
            generateNextPrev: false,
            generatePagination: false
        });
    });
</script>
<!--跑马灯 end-->
