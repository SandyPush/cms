<script>
$(document).ready(function(){
        //计算内容数量
        tabDiv=$("#spvideotab li");
        $("i.gray99").html("("+tabDiv.length+")");
        
        //计算总播放PV
        var contentPv=0;
        $("#spvideotab li").each(function(i){
            contentPv+=parseInt($(this).attr("data"));
        });
        d = new Date();
        t = (Math.ceil(d.getTime()/1000)-parseInt($("#dateline").html()))/86400;
        if(t<1) t=1;
        if(t<=10) decline=0.8;
        if(t>10 && t<30) decline=0.6;
        if(t>=30) decline=0.5;
        contentPv=Math.ceil(t*contentPv*decline);
        $("#bofangshu").html("<i>总播放：</i>"+contentPv);
});
</script>
