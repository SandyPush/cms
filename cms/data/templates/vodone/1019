<script type="text/javascript">
$(document).ready(function() {
var aList=$("li.search").find("a");
	aList.each(function(){
		var hrefValue=$(this).attr("href");
		if(hrefValue.substr(0,22)!='http://video.baidu.com'){
			var value=encodeURI($(this).text());
			$(this).attr("href","http://ynews.v1.cn/news/search/s.jhtml?searchWord="+value); 
		}
	});
});
</script>
