var curUrl = document.location.href;
var search = '?'+curUrl.split('?') [1];

reg=/[0-1a-zA-Z]+\.g\.v1\.cn/i;
if(search){
  $("a").each(function(){
    href=$(this).attr('href');
    if(href && href.match(reg)){
        if(href.indexOf('?')==-1){
          $(this).attr("href",href+search);
        }else{
          $(this).attr("href",href+search.replace('?','&'));
        }
    }
  });
}
</script>
