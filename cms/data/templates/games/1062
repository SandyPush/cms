<html>
<head>
    <title>游戏平台</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="http://static.v1.cn/gplatform/v1.0/css/common.css" charset="utf-8" rel="stylesheet" />
    <link href="http://static.v1.cn/gplatform/v1.0/css/login.css" charset="utf-8" rel="stylesheet" />
    <script src="http://pic.v1.cn/cms/v1_static(new)/common/js/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script src="http://static.v1.cn/gplatform/v1.0/js/common.js" type="text/javascript"></script>    
    <script src="http://pic.v1.cn/cms/v1_static(new)/common/js/passport.js"></script>
<script type="text/javascript">
$(function () {
var ok1 = false;
var ok2 = false;
var ok3 = false;
var ok4 = false;
var partern = /^[0-9a-zA-Z]{6,20}$/;
// 验证用户名
$('#username').focus(function () {
  $(this).next().text('用户名应该为3-20位之间');
}).blur(function () {
  // $('#userTips').text('ok');
  var username = $(this).val();
  var result = partern.test(username);
  if ((result == true) && username != '') {
    uname = $(this).val();
    
    $.getJSON('http://dev.v1cn/Gplatform/lunan/website/interface?mode=jsonp&jsoncallback=?', {
      'obj': 'user.checkUsername',
      'username': uname
    }, function (dataStr) {
      var datas = $.parseJSON(dataStr);
      var result = datas.result.data.items.result;
      if (result == 1)
      {
        $('#userTips').text('用户已存在');
        $('#userTips').css('color', 'red');
      } 
      else
      {
        $('#userTips').text('6~20位字母和数字组成的用户名');
        $('#userTips').css('color', 'green');
      }
    });
    ok1 = true;
  } else {
    $('#userTips').text('6~20位字母和数字组成的用户名');
    $('#userTips').css('color', 'red');
  }
});
//验证密码
$('#password').focus(function () {
  $('#pwdTips').text('6~20位字母和数字组成的登录密码');
}).blur(function () {
  var password = $(this).val();
  var result = partern.test(password);
  if ((result == 1) && password != '') {
    $('#pwdTips').text('6~20位字母和数字组成的登录密码');
    $('#pwdTips').css('color', 'green');
    ok2 = true;
  } else {
    //alert('错误');
    $('#pwdTips').text('6~20位字母和数字组成的登录密码');
    $('#pwdTips').css('color', 'red');
  }
});
//验证确认密码
$('#repwd').focus(function () {
  $('#conTips').text('6~20位字母和数字组成的登录密码');
}).blur(function () {
  var repwd = $(this).val();
  var result = partern.test(repwd);
  if ((result == true) && repwd != '' && (repwd == $('input[name="password"]').val())) {
    $('#conTips').text('6~20位字母和数字组成的登录密码');
    $('#conTips').css('color', 'green');
    ok3 = true;
  } else {
    $('#conTips').text('请确认和上一次输入的密码一致！');
    $('#conTips').css('color', 'red');
  }
});
$('#code').focus(function () {
  $('#codeTips').text('请输入验证码');
}).blur(function () {
  if ($(this).val() != '')
  {
    alert($(this).val());
    if (ok1 && ok2 && ok3)
    {
      uname = $('#username').val();
      pwd = $('#password').val();
      code = $('#code').val();
      regObj = {
        username: uname,
        password: pwd,
        captchaCode: code,
        authid: 'v1002',
        mac: ''
      };
      //console.log(regObj);
      reghelloservice(regObj, function (callbackJson) {
        var regCallback = callbackJson;
        status = regCallback.status;
        message = regCallback.message;
       
        if (status == 0)
        {
          $('#codeTips').text('验证码输入正确');
          $('#codeTips').css('color','green');
          ok4 = true;
        } 
        else
        {
          $('#codeTips').text(message);
          $('#codeTips').css('color','red');
          ok4=false;
        }
      });
    } 
    else
    {
      alert('请将用户和密码输入正确');
    }    
  } else {
    $('#codeTips').text('请输入验证码');
  }
});
$('#buttonReg').click(function () {
  if (ok1 && ok2 && ok3 & ok4) {
    alert('注册成功');
    window.location.href = '/index.shtml';
  } else {
    alert('注册失败！');
    return false;
  }
});

});
</script>
</head>
