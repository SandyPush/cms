    <script src="http://static.v1.cn/gplatform/v1.0/js/common.js" type="text/javascript"></script>
    <script src="http://pic.v1.cn/cms/v1_static(new)/common/js/passport.js"></script>
    <script type="text/javascript">
$(function(){
$('.login_captcha').css('visibility', 'hidden');
$('#loginButton').click(function () {
  var uname = $('#username').val();
  var pwd = $('#password').val();
  var code = $('#captcha').val();
  if(uname=='' && pwd=='') 
  {
    alert('请输入用户名和密码!');
    return false;
  }
  isCookie = typeof ($('#cookieID:checked').val()) == 'undefined' ? 0 : $('#cookieID:checked').val();		
if(isCookie == 'on')
{
	failuretime = 43200*2*31;
}
else
{
	failuretime = -1;
}
  loginObj = {
    username: uname,
    password: pwd,
    captchaCode: code,
    failtime: failuretime,
    authid: 'v1002'
  };
  helloservice(loginObj, function (callbackJson) {
    var datas = callbackJson;
    status = datas.status;
    message = datas.message;
    if (status == 1) {
      alert('用户名、密码输入错误，请重新输入');
    } 
    else if (status == 2)
    {
      $('.login_captcha').css('visibility', 'visible');
    } 
    else
    {
      alert('登录成功');
      window.location.href ="/index.shtml";
    }
  });
});

});
    </script>
    
</head>
