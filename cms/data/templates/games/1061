$(function(){
        $('#qqLogin').click(function(){
	   oauthqq();
        });

        //新浪微博登录
        $('#sinaLogin').click(function(){
	    oauthsinawb();
        });

	$('.login_captcha').css('visibility', 'hidden');
        $("#changeCode_reg").click(function(){
        $(".regist_code").attr("src","http://pass.g.v1.cn/main/showvcode.do?r="+Math.round(Math.random()*100000));
        });
	$('#loginButton').click(function () {
	var uname = $('#username').val();
	var pwd = $('#password').val();
	var code = $('#captcha').val();
	if(uname=='' && pwd=='') 
	{
	alert('请输入用户名和密码!');
	return false;
	}
	isCookie = typeof ($('#cookieID:checked').val()) == 'undefined' ? 0 : $('#cookieID:checked').val();		
	if(isCookie == 'on')
	{
		failuretime = 43200*2*31;
	}
	else
	{
		failuretime = -1;
	}
	loginObj = {
	username: uname,
	password: pwd,
	captchaCode: code,
	failtime: failuretime,
	authid: 'v1002'
	};
	helloservice(loginObj, function (callbackJson) {
	var datas = callbackJson;
			var status = datas.status;
			var msg = datas.message;
					   
			if(status == 1)
			{
				alert(msg);
			   
			}
			else if(status==2)
			{
				$('.login_captcha').css('visibility','visible');
				alert(msg);
			}
			else
			{
				ssocookie = datas.data.params;
				ssocookie=ssocookie.replace(/\//g,'_');
				$.getJSON(platformInterface+'?mode=jsonp&jsoncallback=?', {'obj': 'user.login','ssocookie':ssocookie}, function (dataStr){
					alert('登录成功！');
					window.location.href = '/index.shtml'; 
				});
			}			
	});
});
});
