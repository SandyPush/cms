            var status = datas.status;
            var msg = datas.message;
                       
            if(status == 1)
            {
                alert('请输入正确的用户名和密码');
               
            }
            else if(status==2)
            {
                $('.login_captcha').css('visibility','visible');
                alert(msg);
            }
            else
            {
                ssocookie = datas.data.params;
                ssocookie=ssocookie.replace(/\//g,'_');
                $.getJSON(platformInterface+'?mode=jsonp&jsoncallback=?', {'obj': 'user.login','ssocookie':ssocookie}, function (dataStr){
                    login(dataStr);
                });
            }			
  });
});

});
