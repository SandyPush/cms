<script>
  $(document).ready(function(){
    timeLocalStr=$("#timeLocal").html();
    timeLocalArray=timeLocalStr.split(',');
    var date1=new Date(timeLocalArray[0],timeLocalArray[1]-1,timeLocalArray[2],timeLocalArray[3],timeLocalArray[4],timeLocalArray[5]);  //开始时间
    timeLocalUnix=date1.getTime()/1000;

    $("#mainContent li").each(function(){
      activitieUnix=parseInt($(this).find("#activitieTime").html());
      if(activitieUnix-timeLocalUnix<0){
        $(this).find("#activitieButton").html('活动结束').css("background-color","silver").attr("href","#").click(function(){
          alert('活动已经结束，请参与其他活动！');
        });
      }  
    });

    //搜索
    $("#searchActivitie").click(function(){
      keyword=$("#searchText").val();
      if(keyword){
        $("#mainContent a.game_desc_a").each(function(i){
          desc=$(this).html();
          if(desc.indexOf(keyword)==-1){
            $("#mainContent li.events_module_li").eq(i).hide();
          }else{
            $("#mainContent li.events_module_li").eq(i).show();
          }
        });
      }
    });
  });
</script>
<body>
